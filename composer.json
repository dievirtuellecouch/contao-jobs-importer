    "type": "contao-bundle",
    "version": "0.1.0",
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13",
        "plenta/contao-jobs-basic-bundle": "2.*",
        "symfony/serializer": "^6.4",
        "dazet/data-map": "^0.2.0"
    },
    "conflict": {
        "plenta/contao-jobs-basic-bundle": "<2.4"
    },
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Jonathan Hermann",
            "email": "jh@d-v-c.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "DVC\\JobsImporterToPlentaBasic\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "DVC\\JobsImporterToPlentaBasic\\ContaoManager\\Plugin"
    }
